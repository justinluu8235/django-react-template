{
  "name": "django-react-template",
  "version": "1.0.0",
  "description": "A template for a Django project with a React frontend. Supports the following: 1. Django backend (along with Django REST Framework) 2. React with TypeScript support 3. My preferred styling setup 4. Necessary scaffolding for elastic beanstalk deployment",
  "main": "webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "webpack-dev-server --config webpack.config.js",
    "build": "webpack --config webpack.config.js --mode production",
    "tailwind:dev": "tailwindcss -i ./frontend/static/styles/input.css -o ./frontend/dist/output.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prashanthselvam/django-react-template.git"
  },
  "author": "prashanthselvam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prashanthselvam/django-react-template/issues"
  },
  "homepage": "https://github.com/prashanthselvam/django-react-template#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "babel-loader": "^9.1.2",
    "tailwindcss": "^3.2.4",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
